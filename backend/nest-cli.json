{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/backend",
  "projects": {
    "account": {
      "type": "application",
      "root": "apps/account",
      "entryFile": "main",
      "sourceRoot": "apps/account/src",
      "compilerOptions": {
        "tsConfigPath": "apps/account/tsconfig.app.json"
      }
    },
    "agent": {
      "type": "application",
      "root": "apps/agent",
      "entryFile": "main",
      "sourceRoot": "apps/agent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/agent/tsconfig.app.json"
      }
    },
    "backend": {
      "type": "application",
      "root": "apps/backend",
      "entryFile": "main",
      "sourceRoot": "apps/backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backend/tsconfig.app.json"
      }
    },
    "cache": {
      "type": "library",
      "root": "libs/cache",
      "entryFile": "index",
      "sourceRoot": "libs/cache/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cache/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "credit": {
      "type": "library",
      "root": "libs/credit",
      "entryFile": "index",
      "sourceRoot": "libs/credit/src",
      "compilerOptions": {
        "tsConfigPath": "libs/credit/tsconfig.lib.json"
      }
    },
    "liquidity-management": {
      "type": "application",
      "root": "apps/liquidity-management",
      "entryFile": "main",
      "sourceRoot": "apps/liquidity-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/liquidity-management/tsconfig.app.json"
      }
    },
    "liquidity-pool": {
      "type": "library",
      "root": "libs/liquidity-pool",
      "entryFile": "index",
      "sourceRoot": "libs/liquidity-pool/src",
      "compilerOptions": {
        "tsConfigPath": "libs/liquidity-pool/tsconfig.lib.json"
      }
    },
    "redis-price": {
      "type": "library",
      "root": "libs/redis-price",
      "entryFile": "index",
      "sourceRoot": "libs/redis-price/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis-price/tsconfig.lib.json"
      }
    },
    "sui": {
      "type": "library",
      "root": "libs/sui",
      "entryFile": "index",
      "sourceRoot": "libs/sui/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sui/tsconfig.lib.json"
      }
    },
    "svm": {
      "type": "library",
      "root": "libs/svm",
      "entryFile": "index",
      "sourceRoot": "libs/svm/src",
      "compilerOptions": {
        "tsConfigPath": "libs/svm/tsconfig.lib.json"
      }
    },
    "wallet-management": {
      "type": "application",
      "root": "apps/wallet-management",
      "entryFile": "main",
      "sourceRoot": "apps/wallet-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/wallet-management/tsconfig.app.json"
      }
    }
  }
}